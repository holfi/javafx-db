<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.store.LoginController">
<children>
    <TextField fx:id="textLogin" layoutX="25.0" layoutY="56.0" prefHeight="25.0" prefWidth="261.0" promptText="Введите логин" />
    <Label layoutX="29.0" layoutY="29.0" text="Логин">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Label layoutX="29.0" layoutY="97.0" text="Пароль">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Button fx:id="loginButton" layoutX="175.0" layoutY="195.0" mnemonicParsing="false" onAction="#loginHandle" prefHeight="30.0" prefWidth="111.0" text="Войти">
        <font>
            <Font size="18.0" />
        </font>
    </Button>
      <PasswordField fx:id="passwordLogin" layoutX="25.0" layoutY="128.0" prefHeight="25.0" prefWidth="261.0" promptText="Введите пароль" />
</children>
<padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</padding>
</AnchorPane>
